BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//{{.Site.Title}}//{{i18n "ical"}}//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
{{range $index, $item := .Pages -}}
BEGIN:VEVENT
ORGANIZER;CN="{{.Site.Params.calendarOrganizer}}":mailto:{{.Site.Params.contact}}
{{range $item.Params.speakers -}}
ATTENDEE;CN="{{.name}}";CUTYPE=INDIVIDUAL;PARTSTAT=ACCEPTED:mailto:{{.email}}
{{end -}}
SUMMARY:{{$item.Title}}
UID:{{dateFormat "20060102T150405Z" $item.Date.UTC}}@{{$index}}@{{.Site.Params.calendarUidSuffix}}
SEQUENCE:0
STATUS:CONFIRMED
DTSTAMP:{{dateFormat "20060102T150405Z" $item.Date.UTC}}
DTSTART:{{dateFormat "20060102T150405Z" $item.Date.UTC}}
DTEND:{{dateFormat "20060102T150405Z" (time $item.Params.end).UTC}}
{{ $roomNames := slice "Unknown" (i18n "room_intlroom") (i18n "room_large") (i18n "room_mid")}}
LOCATION:{{ index $roomNames ($item.Params.room) }}
DESCRIPTION:{{$item.Content}}
URL:{{$item.Permalink}}
END:VEVENT
{{end -}}
END:VCALENDAR
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//{{.Site.Title}}//{{i18n "ical"}}//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
{{range .Pages -}}
BEGIN:VEVENT
ORGANIZER;CN="{{.Site.Params.calendarOrganizer}}({{.Site.Params.contact}})"
SUMMARY:"{{.Title}} ({{range .Params.speakers -}}{{.name}}; {{end -}})"
UID:{{dateFormat "20060102T150405Z" .Date.UTC}}@{{.Site.Params.calendarUidSuffix}}
SEQUENCE:0
STATUS:CONFIRMED
DTSTAMP:{{dateFormat "20060102T150405Z" .Date.UTC}}
DTSTART:{{dateFormat "20060102T150405Z" .Date.UTC}}
DTEND:{{dateFormat "20060102T150405Z" (time .Params.end).UTC}}
{{ $roomNames := slice "Unknown" (i18n "room_intlroom") (i18n "room_mid") (i18n "room_mid")}}
LOCATION:{{ index $roomNames (.Params.room) }}
DESCRIPTION:{{.Content}}
URL:{{.Permalink}}
END:VEVENT
{{end -}}
END:VCALENDAR